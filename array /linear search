#include <stdio.h>

int main()
{
    int elemnt = 42;
    int k = 0;
    int a[] = {1, 2, 3, 4, 5, 42, 34, 56};

    for (int i = 0; i < 8; i++)
    {
        if (a[i] == elemnt)
        {
            printf("%d\n", elemnt);
            k = 1; // Added a newline character here
            break;
        }

        // else if(a[i] != elemnt) {
        //     printf("not found elemnt");  // Added a newline character here
        //     break;
        // }
    }

    if (k == 0)
    {
        printf("not found ");
    }

    return 0;
}
